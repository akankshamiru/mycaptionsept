#include<iostream>
using namespace std;
int main()
{
	int marks;
	cout<<"Enter the marks obtained by the student: ";
	cin>>marks;
	if(marks>=80)
	{
		cout<<"\nGrade A\n";
	}
	else if(marks>=60 && marks<=80)
	{
		cout<<"Grade B\n";
	}
	else if(marks>=40 && marks<=60)
	{
		cout<<"Grade C\n";
	}
	else 
	{
		cout<<"Grade D\n";
	}
	return 0;
}
